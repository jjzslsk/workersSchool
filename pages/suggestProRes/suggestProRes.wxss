/* pages/suggestProRes/suggestProRes.wxss */
.body-bg{
  width: 100vw;
  height: 100vh;
}

.scroll-area{
  height: 100%;
  background-color: #f5f5f5;
}

.pro-res-item{
  padding: 8px 10px;
  background-color: #FFFFFF;
}

.pro-res-item .pro-res-item-row{
  display: flex;
  margin-top: 5px;
}

.pro-res-item .pro-res-item-row .title{
  width: 5em;
}

.pro-res-item .pro-res-item-row .content{
  width: calc(100% - 5em);
  color:#999999;
}